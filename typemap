uv_run_mode         T_RUN_UV
uv_loop_option      T_RUN_UV

int64_t             T_INT64
uint64_t            T_UINT64

uv_loop_t *         T_PTROBJ_MG
uv_handle_t *       T_PTROBJ_MG

INPUT

T_RUN_UV
    $var = SvIV($arg)

T_INT64
    $var = SvI64($arg);

T_UINT64
    $var = SvU64($arg);

OUTPUT

T_INT64
